var map_loaded=false;document.getElementById('modal-map').addEventListener('click',function(){if(typeof $!=='undefined'){if(map_loaded===false){$('#map-modal').one('shown.bs.modal',function(){var map=new BMap.Map('cngb-map');var point=new BMap.Point(114.472274,22.592458);map.centerAndZoom(point,17);var marker=new BMap.Marker(point);map.addOverlay(marker);map.enableScrollWheelZoom(true);map.addControl(new BMap.NavigationControl());map.addControl(new BMap.ScaleControl());map.addControl(new BMap.OverviewMapControl());map.addControl(new BMap.MapTypeControl());map.setCurrentCity("深圳");map_loaded=true;});};$('#map-modal').modal('show');};});;